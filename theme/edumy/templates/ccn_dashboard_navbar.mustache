{{{custom_css_dashboard}}}
<header class="header-nav menu_style_home_one dashbord_pages navbar-scrolltofixed stricky main-menu">
  <div class="container-fluid">
    <nav>
      <div class="menu-toggle">
        <img class="nav_logo_img img-fluid" src="{{{headerlogo1}}}" alt="{{{sitename}}}">
        <button type="button" id="menu-btn">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
      </div>
      {{#logo}}
        <a href="{{{ config.wwwroot }}}" class="navbar_brand float-left dn-smd">
          {{#logo_image}}<img class="logo1 img-fluid" src="{{{headerlogo1}}}" alt="{{{sitename}}}">{{/logo_image}}
          {{#logo_image}}<img class="logo2 img-fluid" src="{{{headerlogo1}}}" alt="{{{sitename}}}">{{/logo_image}}
          {{#logotype}}<span>{{{ sitename }}}</span>{{/logotype}}
        </a>
      {{/logo}}
      <ul id="respMenu" class="ace-responsive-menu" data-menu-style="horizontal">
        {{{ output.custom_menu }}}
      </ul>
      {{> theme_edumy/ccn_navbar_user }}
    </nav>
  </div>
</header>
<div id="page" class="stylehome1 h0">
  <div class="mobile-menu">
    <div class="header stylehome1 dashbord_mobile_logo dashbord_pages">
      <div class="main_logo_home2">
        <img class="nav_logo_img img-fluid float-left mt20" src="{{{headerlogo1}}}" alt="{{{sitename}}}">
        <span>{{{sitename}}}</span>
      </div>
      <ul class="menu_bar_home2">
        <li class="list-inline-item"></li>
        <li class="list-inline-item"><a href="#menu"><span></span></a></li>
      </ul>
    </div>
  </div><!-- /.mobile-menu -->
  <nav id="menu" class="stylehome1">
    <ul>
      {{{ output.custom_menu }}}
      {{> theme_edumy/ccn_header_mob_user }}
    </ul>
  </nav>
</div>
